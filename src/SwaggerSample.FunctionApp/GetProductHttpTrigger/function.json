{
  "disabled": false,

  "scriptFile": "..\\bin\\SwaggerSample.FunctionApp.dll",
  "entryPoint": "SwaggerSample.FunctionApp.GetProductHttpTrigger.Run",

  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "products/{productId}",
      "authLevel": "function",
      "methods": [
        "get"
      ]
    },
    {
      "name": "return",
      "type": "http",
      "direction": "out"
    }
  ]
}